{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "apps/**/build/**",
        "packages/**/build/**"
      ]
    },
    "codecheck": {
      "persistent": false,
      "cache": false
    },
    "typecheck": {
      "persistent": false,
      "cache": false
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "prod": {
      "persistent": true,
      "cache": false
    },
    "lint": {
      "persistent": true,
      "cache": false
    },
    "test": {
      "persistent": true,
      "cache": false
    }
  }
}